<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <decisions>
        <name>is_GW_greater_CBM</name>
        <label>is GW &gt;= CBM ?</label>
        <locationX>534</locationX>
        <locationY>323</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>GW_CBM1000</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.TransportationWay__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>SEA</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.TransportationWay__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>INLAND WATERWAY</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.GrossWeight__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>CBMQuyDoi1000</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>update_CW_as_GW</targetReference>
            </connector>
            <label>GW &gt;= CBM1000</label>
        </rules>
