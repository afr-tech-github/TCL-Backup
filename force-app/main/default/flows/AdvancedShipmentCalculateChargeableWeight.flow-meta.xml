    </formulas>
    <formulas>
        <name>CBMQuyDoi5000</name>
        <dataType>Number</dataType>
        <expression>CASE( {!$Record.Air_Type__c} ,
&apos;Air Express&apos;, {!$Record.MeasurementCBM__c}*5000 ,
&apos;Air Cargo&apos;, {!$Record.MeasurementCBM__c}*6000 ,
0)</expression>
