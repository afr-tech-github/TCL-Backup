        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Measurement_TotalCBM_is_changed</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.MeasurementCBM__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.GrossWeight__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RollupGrossWeight__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.RollupCBM__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_all_Consol_Bills</targetReference>
            </connector>
            <label>Measurement TotalCBM is changed</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>QuantityCBM</name>
        <dataType>Number</dataType>
        <expression>CASE({!$Record.RecordType.DeveloperName},
	&quot;Group&quot;,
		IF(({!$Record.MeasurementCBM__c}*1000)&gt;{!$Record.GrossWeight__c},
			IF({!$Record.MeasurementCBM__c} &lt;= 1,1,{!$Record.MeasurementCBM__c}),
				IF({!$Record.GrossWeight__c}/1000 &lt; 1,1,{!$Record.GrossWeight__c}/1000)),
		
	&quot;Consol&quot;,
		IF(({!$Record.RollupCBM__c}*1000)&gt;{!$Record.RollupGrossWeight__c},
			IF({!$Record.RollupCBM__c} &lt;= 1,1,{!$Record.RollupCBM__c}),
				IF({!$Record.RollupGrossWeight__c}/1000 &lt; 1,1,{!$Record.RollupGrossWeight__c}/1000)),NULL)</expression>
        <scale>3</scale>
    </formulas>
