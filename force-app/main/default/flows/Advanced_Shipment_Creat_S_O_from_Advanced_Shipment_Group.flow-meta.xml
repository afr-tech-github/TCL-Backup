    <formulas>
        <name>NumberHBLsUpdate</name>
        <dataType>Number</dataType>
        <expression>IF({!NumberOfSO} &gt; 0, {!NumberOfSO}, 0) + 1</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>ShipmentRecordTypeFormula</name>
        <dataType>String</dataType>
        <expression>CASE({!Get_Advanced_Shipment.Master_Record_Type__c},
    &quot;Ocean Export&quot;,&quot;OceanExport&quot;,
	&quot;Ocean Import&quot;,&quot;OceanImport&quot;,
	NULL)</expression>
    </formulas>
    <formulas>
        <name>SoURL</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_450}, FIND(&apos;/services&apos;, {!$Api.Partner_Server_URL_450})) + CASESAFEID({!varSingleSO.Id})</expression>
    </formulas>
