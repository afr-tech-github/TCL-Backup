<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CannotnullComboProduct</fullName>
    <active>false</active>
    <errorConditionFormula>AND(  ISNULL(ComboProduct__c) ,NOT(ISNULL( InquiryLine__r.ComboProduct__c )))</errorConditionFormula>
    <errorDisplayField>ComboProduct__c</errorDisplayField>
    <errorMessage>Please input Combo Product as same as Combo Product in Inquiry Line</errorMessage>
</ValidationRule>
