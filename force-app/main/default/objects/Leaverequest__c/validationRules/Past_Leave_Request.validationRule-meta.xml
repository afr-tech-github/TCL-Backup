<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Past_Leave_Request</fullName>
    <active>false</active>
    <errorConditionFormula>OR (Start_Date__c &lt; Today(),
 End_Date__c &lt; Today())</errorConditionFormula>
    <errorMessage>Leave Request cannot be in the past!</errorMessage>
</ValidationRule>
