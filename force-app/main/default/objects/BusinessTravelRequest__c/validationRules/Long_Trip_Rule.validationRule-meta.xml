<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Long_Trip_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
 LongTrip__c = False,
 From__c &lt;&gt; To__c)</errorConditionFormula>
    <errorMessage>Short trip cannot have multiple travel days!</errorMessage>
</ValidationRule>
